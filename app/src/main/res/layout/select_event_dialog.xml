                app:layout_constraintEnd_toStartOf="@+id/first_line_tick"
                app:layout_constraintHorizontal_bias="0.12"
                app:layout_constraintStart_toEndOf="@+id/first_line"
                app:layout_constraintTop_toTopOf="parent" />

